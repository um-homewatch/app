{
  "rulesDirectory": [
    "node_modules/codelyzer"
  ],
  "extends": [
    "angular-tslint-rules",
    "tslint-ionic-rules"
  ],
  "rules": {
    "quotemark": [
      "double"
    ],
    "switch-default": false,
    "typedef": [
      false
    ],
    "curly": false,
    "max-line-length": [
      false
    ],
    "component-class-suffix": false,
    "component-selector": [
      false
    ],
    "no-access-missing-member": [
      false
    ]
  }
}
